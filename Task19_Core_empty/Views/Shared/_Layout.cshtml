
<!DOCTYPE html>
<html lang="ru">
<head >
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>My Application Phone Book</title>



    <link rel="stylesheet" href="~/css/style1.css?v=1.1" asp-append-version="true" />
    @await RenderSectionAsync("CustomCSS", required: false)
</head>
<body>
    <header class="page-container header">

        <div class="header__container flex">
            <a class="a-reset header__company-link" href="#">
                <img class="header__logo-img" src="~/img/phonebook.svg" alt="логотип phone book">
            </a>
            <div class="header__con-nav con-nav flex">
                <nav class="nav con-nav__nav">
                    <ul class="nav__list list-reset flex">
                        <li class="nav__item">
                            <a class="a-reset nav__link nav__link_action" href="#about-us">
                                О нас
                            </a>
                        </li>
                        <li class="nav__item">
                            <a class="a-reset nav__link nav__link_action" href="#projects">
                                PhoneBook
                            </a>
                        </li>
                        
                        <li class="nav__item">
                            <a class="a-reset nav__link nav__link_action" href="#review">
                                Отзывы
                            </a>
                        </li>

                    </ul>
                </nav>
               
                <a class="btn-reset  con-nav__btn-search btn-search" href="#search" aria-label="открыть форму поиск">
                    <svg class="btn-search__svg" width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <rect width="7.56126" height="1.89031" transform="matrix(0.713342 0.700816 -0.713342 0.700816 18.6064 17.3762)" />
                        <path d="M20.9303 10.7726C20.9303 16.1533 16.4855 20.5452 10.9652 20.5452C5.44478 20.5452 1 16.1533 1 10.7726C1 5.39198 5.44478 1 10.9652 1C16.4855 1 20.9303 5.39198 20.9303 10.7726Z" stroke-width="2" />
                    </svg>
                </a>

                <form class="search" id="#search">
                    <input class="search__input" type="text" placeholder="Что будем искать?">
                    <button class="btn-reset search__btn-search" type="button" aria-label="искать">
                        <svg class="btn-search__svg" width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <rect width="7.56126" height="1.89031" transform="matrix(0.713342 0.700816 -0.713342 0.700816 18.6064 17.3762)" />
                            <path d="M20.9303 10.7726C20.9303 16.1533 16.4855 20.5452 10.9652 20.5452C5.44478 20.5452 1 16.1533 1 10.7726C1 5.39198 5.44478 1 10.9652 1C16.4855 1 20.9303 5.39198 20.9303 10.7726Z" stroke-width="2" />
                        </svg>
                    </button>
                    <a class="a-reset search__btn-cancel" href="#" aria-label="поиск отмена">
                        <svg class="search-btn-cancel__svg" width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <rect x="8.95386" y="7.30444" width="11.6645" height="2.33291" transform="rotate(45 8.95386 7.30444)" />
                            <rect x="7.3042" y="15.5525" width="11.6645" height="2.33291" transform="rotate(-45 7.3042 15.5525)" />
                            <circle cx="12" cy="12" r="11" stroke-width="2" />
                        </svg>

                    </a>
                </form>

            </div>
        </div>

    </header>
    <main class="page-container">
        <section class="section projects" id="projects">
            <div>
                <a href="#" id="d" class="d">hello Link</a>
                @RenderBody()
            </div>
    </section>
    </main>
    <script src="~/js/script.js" type="text/javascript"></script>
    
</body>
</html>
